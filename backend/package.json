    "@types/express": "^5.0.1",
    "@types/node": "^22.5.5",
    "cross-env": "^7.0.3",
    "tsx": "^4.19.4",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
